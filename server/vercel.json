{
    "name": "api-task-manager",
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/help",
            "status": 204
        },
        {
            "src": "/(.*)",
            "dest": "/src/server.ts"
        }
    ]
}